<?xml version="1.0"?>
<facelet-taglib version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd">
 <namespace>http://jsf.ranga.com/customtags</namespace>
 <tag>
  <description>Credit Card Convertor</description>
  <tag-name>creditCardConvertor</tag-name>
  <converter>
   <converter-id>com.ranga.jsf.convertor.CreditCardConvertor</converter-id>
  </converter>
  <attribute>
   <name>seperator</name>
   <type>java.lang.String</type>
  </attribute>
 </tag>
 <tag>
  <description>Credit Card Validator</description>
  <tag-name>creditCardValidator</tag-name>
  <validator>
   <validator-id>com.ranga.jsf.validator.CreditCardValidator</validator-id>
  </validator>
  <attribute>
   <name>errorMessage</name>
   <type>java.lang.String</type>
  </attribute>
 </tag>
</facelet-taglib>
